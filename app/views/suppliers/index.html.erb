<p id="notice"><%= notice %></p>

<table>
  <thead>
    <tr>
        <th>
            <%= t('activerecord.attributes.supplier.company') %>
        </th>
        <th>
            <%= t('activerecord.attributes.supplier.name') %>
        </th>
        <th>
            <%= t('activerecord.attributes.supplier.address') %>
        </th>
        <th>
            <%= t('activerecord.attributes.supplier.phone') %>
        </th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @suppliers.each do |supplier| %>
      <tr>
        <td><%= supplier.company %></td>
        <td><%= supplier.name %></td>
        <td><%= supplier.address %></td>
        <td><%= supplier.phone %></td>
        <td><%= link_to t('Show'), supplier %></td>
        <td><%= link_to t('Edit'), edit_supplier_path(supplier) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to t('add'), new_supplier_path %>
